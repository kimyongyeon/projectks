<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:gravity="center"
  android:padding="10dip"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  	<ImageView
    	android:id="@+id/photoboardlistRow_Iv01"
    	android:padding="10dip"
    	android:layout_width="50dip"
    	android:layout_height="50dip"
    />
    <LinearLayout
		android:orientation="vertical"
		android:gravity="left"
		android:layout_weight="1"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
	    <TextView
	    	android:id="@+id/photoboardlistRow_Tv01"
	    	android:textColor="#000000"
	    	android:text="글제목............"
	    	android:layout_weight="1"
	    	android:layout_height="wrap_content" 
	    	android:layout_width="fill_parent"/>
	    <TextView
	    	android:id="@+id/photoboardlistRow_Tv02"
	    	android:textColor="#000000"
	    	android:text="2011-10-22"
	    	android:layout_weight="1"
	    	android:layout_height="wrap_content" 
	    	android:layout_width="fill_parent"/>
    </LinearLayout>
    <ImageView
    	android:id="@+id/photoboardlistRow_Iv02"
    	android:background="@drawable/tablecell_arrow"
    	android:layout_width="10dip"
    	android:layout_height="10dip"
    />
</LinearLayout>
